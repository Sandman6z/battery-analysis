{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Qt Designer",
            "type": "shell",
            "command": ".venv\\Lib\\site-packages\\qt5_applications\\Qt\\bin\\designer.exe",
            "args": [
                "./config/resources/ui/UI_BatteryAnalysis.ui"
            ],
            "problemMatcher": []
        },
        {
            "label": "PyUIC",
            "type": "shell",
            "command": ".venv\\Scripts\\pyuic5.exe",
            "args": [
                "./config/resources/ui/UI_BatteryAnalysis.ui",
                "-o",
                "./src/battery_analysis/ui/ui_main_window.py"
            ],
            "problemMatcher": []
        },
        {
            "label": "Pyrcc",
            "type": "shell",
            "command": ".venv\\Scripts\\pyrcc5.exe",
            "args": [
                "./config/resources/icons/resources.qrc",
                "-o",
                "./src/battery_analysis/resources_rc.py"
            ],
            "problemMatcher": []
        },
        {
            "label": "(venv)build debug version",
            "type": "shell",
            "command": ".venv\\Scripts\\Activate.ps1",
            "args": [
                "&&",
                "python",
                "-m",
                "scripts.build",
                "Debug"
            ],
            "problemMatcher": [
                "$python"
            ]
        },
        {
            "label": "(venv)build release version",
            "type": "shell",
            "command": ".venv\\Scripts\\Activate.ps1",
            "args": [
                "&&",
                "python",
                "-m",
                "scripts.build",
                "Release"
            ],
            "problemMatcher": [
                "$python"
            ]
        }
    ]
}